<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
    <!-- 配置文件路径  -->
    <properties resource="generator/generator.properties"/>
    <!-- 数据库驱动:选择你的本地硬盘上面的数据库驱动包-->
    <!--<classPathEntry  location="E:\reverse\mysql-connector-java-5.1.28-bin.jar"/>-->
    <classPathEntry  location="D:\reverse\mysql-connector-java-5.1.28-bin.jar"/>
    <context id="DB2Tables"  targetRuntime="MyBatis3">
        <!-- 为生成的Java模型类添加序列化接口，并生成serialVersionUID字段 -->
        <!--<plugin type="org.mybatis.generator.plugins.SerializablePlugin" />-->
        <!--添加Mapper注解-->
        <!--<plugin type="org.mybatis.generator.plugins.MapperAnnotationPlugin" />-->
        <!-- 代码生成覆盖 -->
        <plugin type="org.mybatis.generator.plugins.UnmergeableXmlMappersPlugin" />
        <!-- 查询单条数据插件 -->
        <plugin type="com.itfsw.mybatis.generator.plugins.SelectOneByExamplePlugin"/>
        <!-- MySQL分页插件 -->
        <plugin type="com.itfsw.mybatis.generator.plugins.LimitPlugin">
            <!-- 通过配置startPage影响Example中的page方法开始分页的页码，默认分页从0开始 -->
            <property name="startPage" value="0"/>
        </plugin>
        <!-- 数据Model属性对应Column获取插件 -->
        <plugin type="com.itfsw.mybatis.generator.plugins.ModelColumnPlugin"/>
        <!-- 批量插入插件 -->
        <plugin type="com.itfsw.mybatis.generator.plugins.BatchInsertPlugin" />
        <!-- Selective选择插入更新增强插件 -->
        <plugin type="com.itfsw.mybatis.generator.plugins.SelectiveEnhancedPlugin"/>
        <!-- 逻辑删除插件 逻辑删除目前在实际工程中有项目冲突 无法正常工作-->
        <!--<plugin type="com.itfsw.mybatis.generator.plugins.LogicalDeletePlugin">-->
            <!--&lt;!&ndash; 这里配置的是全局逻辑删除列和逻辑删除值，当然在table中配置的值会覆盖该全局配置 &ndash;&gt;-->
            <!--&lt;!&ndash; 逻辑删除列类型只能为数字、字符串或者布尔类型 &ndash;&gt;-->
            <!--<property name="logicalDeleteColumn" value="is_valid"/>-->
            <!--&lt;!&ndash; 逻辑删除-已删除值 &ndash;&gt;-->
            <!--<property name="logicalDeleteValue" value="N"/>-->
            <!--&lt;!&ndash; 逻辑删除-未删除值 &ndash;&gt;-->
            <!--<property name="logicalUnDeleteValue" value="Y"/>-->
            <!--&lt;!&ndash; 是否生成逻辑删除常量(只有开启时 logicalDeleteConstName、logicalUnDeleteConstName 才生效) &ndash;&gt;-->
            <!--<property name="enableLogicalDeleteConst" value="true"/>-->
            <!--&lt;!&ndash; 逻辑删除常量名称，不配置默认为 IS_DELETED &ndash;&gt;-->
            <!--<property name="logicalDeleteConstName" value="IS_DELETED"/>-->
            <!--&lt;!&ndash; 逻辑删除常量（未删除）名称，不配置默认为 NOT_DELETED &ndash;&gt;-->
            <!--<property name="logicalUnDeleteConstName" value="NOT_DELETED"/>-->
        <!--</plugin>-->
        <!-- Lombok插件 -->
        <plugin type="com.itfsw.mybatis.generator.plugins.LombokPlugin">
            <!-- @Builder 必须在 Lombok 版本 >= 1.18.2 的情况下 -->
            <property name="@Builder" value="true"/>
            <!-- @NoArgsConstructor 和 @AllArgsConstructor 使用规则和Lombok一致 -->
            <property name="@AllArgsConstructor" value="true"/>
            <property name="@NoArgsConstructor" value="true"/>
        </plugin>
        <!-- 自定义注释插件 -->
        <!--<plugin type="com.itfsw.mybatis.generator.plugins.CommentPlugin">-->
            <!--&lt;!&ndash; 自定义模板路径 &ndash;&gt;-->
            <!--<property name="template" value="generator/comment.ftl" />-->
        <!--</plugin>-->
        <!-- 乐观锁插件 -->
        <!--<plugin type="com.itfsw.mybatis.generator.plugins.OptimisticLockerPlugin" />-->

        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>

        <!--数据库链接URL，用户名、密码 -->
        <jdbcConnection
                driverClass="com.mysql.cj.jdbc.Driver"
                connectionURL="jdbc:mysql://${jdbc.url}/${jdbc.database}?nullCatalogMeansCurrent=true&amp;serverTimezone=UTC"
                userId="${jdbc.user}" password="${jdbc.pwd}">
        </jdbcConnection>

        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
            <property name="useJSR310Types" value="true"/>
        </javaTypeResolver>

        <!-- 生成模型的包名和位置-->
        <javaModelGenerator targetPackage="${jdbc.modular.path}.${jdbc.modular.name}.domain" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>
        <!-- 生成映射文件的包名和位置-->
        <sqlMapGenerator targetPackage="mybatis.promote.${jdbc.modular.name}" targetProject="src/main/resources">
            <property name="enableSubPackages" value="true"/>
        </sqlMapGenerator>
        <!-- 生成DAO的包名和位置-->
        <javaClientGenerator type="XMLMAPPER" targetPackage="${jdbc.modular.path}.${jdbc.modular.name}.mapper" targetProject="src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>
        <!-- 要生成的表 tableName是数据库中的表名或视图名 domainObjectName是实体类名-->
        <table tableName="${jdbc.table}"/>
    </context>

</generatorConfiguration>